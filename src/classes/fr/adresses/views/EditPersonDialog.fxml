      <TextField fx:id="weightField" layoutX="121.0" layoutY="88.0" prefHeight="25.0" prefWidth="207.0" />
      <CheckBox fx:id="womanCheckBox" layoutX="121.0" layoutY="126.0" mnemonicParsing="false" onAction="#handleWomanSelection" text="Women" />
